#include "mediatek/mt6765-disable-unused.dtsi"
#include "mediatek/mt6370.dtsi"
#include "mediatek/rt9471.dtsi"
#include "mediatek/trusty.dtsi"

&mt6370_backlight {
	compatible = "mediatek,mt6372-backlight";
	default-brightness = <4096>;
};

/* regulator always on for GKI test */
&mt_pmic_vaud28_ldo_reg {
	regulator-always-on;
};

&mddriver {
/* for md pmic voltage setting*/
	md_vmodem-supply = <&mt_pmic_vmodem_buck_reg>;
	md_vmodem = <800000 800000>;
};

&mt6370_chg {
	charger = <&mtk_charger_type>; /*mt6370 get online status*/
};

&mtk_charger_type {
	charger = <&mt6370_chg>; /*mt6357 get charger status*/
};

&mt6370 {
	interrupts = <11 IRQ_TYPE_LEVEL_LOW 11 0>;
};
