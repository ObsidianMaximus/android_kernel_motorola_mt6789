		mbox_tbl = < 4 >, /* end index of pin table: mbox 0 */
			< 10 >,  /* end index of pin table: mbox 1 */
			< 3 >,  /* end index of pin table: mbox 2 */
			< 0 >; /* end index of pin table: mbox 3 */

